//Q4:

#include "stdafx.h"
#include <iostream>
#include <vector>
#include <string>
#include <cstring>
#include <sstream>

using namespace std;


int main(int argc, char * argv[])
{
	string v;
	for (int i=0;i<argc;i++)
	{
		istringstream v1(argv[i]);
		v1>>v;
		cout<<v<<endl;
		//v+=" ";
	}
	return 0;
}

