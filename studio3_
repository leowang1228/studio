//Q2:



include "stdafx.h"
#include <iostream>  
using namespace std;

int main(int argc, char * argv[])
{
	int a[2][3][5] = {0};
	for (int i = 0; i < 2;i++)
	{
		for (int j = 0; j < 3; j++)
		{
			for (int k = 0; k < 5; k++)
			{
				a[i][j][k] = i*j*k;
				cout << i << j << k << " " << a[i][j][k] << endl;
			}
		}

	}
	
	return 0;
}


*****************************************************************************
//Q3:

#include "stdafx.h"
#include <iostream> 
#include <vector>
#include <string>

using namespace std;

int main(int argc, char * argv[])
{
	
	vector<string> v;
	for (int i = 0; i<argc; ++i)   //get command lines
		v.push_back(argv[i]);

	for (int k = 0; k < v.size(); k++)  //print out command lines
		cout << v[k] << endl;
	
	return 0;
}

//Output is "studio2"
